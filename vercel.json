{
  "version": 2,
  "builds": [
    { "src": "apps/proxy/api/**/*.js", "use": "@vercel/node" },
    { "src": "apps/assets/esm/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/apps/proxy/api/$1" },
    { "src": "/back-office/(.*)", "dest": "/apps/assets/esm/$1",
      "headers": { "Cache-Control": "no-cache" } }
  ]
}
